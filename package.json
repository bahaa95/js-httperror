{
  "name": "ts-httperror",
  "version": "1.0.2",
  "description": "A package to work with httperror in nodejs",
  "author": "bahaa adel <probahaa9@gmail.com> (https://bahaa-adel.vercel.app)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "http",
    "express",
    "nodejs",
    "error",
    "httperror",
    "typescript",
    "status",
    "ts-httperror"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahaa95/ts-httperror.git"
  },
  "bugs": {
    "url": "https://github.com/bahaa95/ts-httperror/issues"
  },
  "homepage": "https://github.com/bahaa95/ts-httperror#readme",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test:coverage": "npm run test -- --coverage",
    "test:coveralls": "npm run test:coverage && coveralls  < coverage/lcov.info"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "coveralls": "^3.1.1",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
